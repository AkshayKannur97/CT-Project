#:kivy 1.0

WindowManager:
    WindowSplash:
    WindowLogin:
    WindowHome:
    WindowTC1:
    WindowTC2:
    WindowTC3:
    WindowTesting:
    WindowCalibration:
    WindowDiagnostics:


<MyButton@Button>
    font_size : 14
    size_hint : (0.1369, 0.0689)
    font_name : 'DejaVuSans'

    #*********Global Border*********
    canvas.before :
        Color:  
            rgba: 1, 1, 1, 0
        Line:
            width: 0.5
            rectangle: self.x, self.y, self.width, self.height

<MyLabel@Label>
    font_size : 14
    size_hint : (0.1369, 0.0689)
    font_name : 'DejaVuSans'

<LabelH3@Label>
    font_size : 12
    size_hint : (0.4, 0.0689)
    font_name : 'DejaVuSans'

<TextInput>
    font_size : 14
    size_hint : (0.1369, 0.0689)
    font_name : 'DejaVuSans'


<WindowSplash>
    name: "layout_splash"
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'res/img/Background.png'

    Image:
        source: 'res/img/splash.png'
        size: self.texture_size

    Label:
        text: "UNIVERSAL INDICATOR"
        font_size: 24
        pos_hint: {'x': 0, 'y': -0.3}


<WindowLogin>
    name: "layout_login"

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'res/img/Background.png'

    FloatLayout:
        size: root.width, root.height
        canvas.before :
            Color:  
                rgba: 1, 0, 0, 0
            Line:
                width: 0.5
                rectangle: self.x, self.y, self.width, self.height

        MyLabel:
            font_size : 24
            text: "LOGIN"
            pos_hint: {"x":0.01369*30, "y":0.03448*26.5}
            background_color: 1, 1, 1, 0
            color: 1, 1, 1, 1
        MyLabel:
            text: "Username"
            pos_hint: {"x": 0.01369*20, "y": 0.03448*20}
        TextInput:
            id: username
            placeholder: "Username"
            hint: "Username"
            input_type: "text"
            multiline: 0
            pos_hint: {"x": 0.01369*30, "y": 0.03448*20}
        MyLabel:
            text: "Password"
            pos_hint: {"x": 0.01369*20, "y": 0.03448*18}
        TextInput:
            id: password
            placeholder: "Password"
            input_type: "text"
            multiline: 0
            password: 1
            pos_hint: {"x": 0.01369*30, "y": 0.03448*18}
        MyButton:
            text: "Login"
            pos_hint: {"x": 0.01369*30, "y": 0.03448*16}
            on_press: root.login()
        MyLabel:
            id: message
            pos_hint: {"x": 0.01369*30, "y": 0.03448*14}
            color: 1, 0, 0, 1


