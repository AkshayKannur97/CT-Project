                canvas.before :
                    Color:  
                        rgba: 1, 1, 1, .5
                    Line:
                        width: 2
                        rectangle: self.x, self.y, self.width, self.height

            Label:
                text: "CUSTOMER / VENDOR DETAILS"
                pos_hint: {'x': 0, 'y': 0.45}
                font_size: 18
            LabelH3:
                text: "CUSTOMER/VENDOR"
                pos_hint : {"x":0.05, "y":0.8}
                halign: 'center'
                background_color :182, 182, 182, 0.3
                canvas.before:
                    Color:
                        rgba: self.background_color
                    Rectangle:
                        size: self.size
                        pos: self.pos
