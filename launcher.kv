                halign: 'center'
                background_color :182, 182, 182, 0.3
                canvas.before:
                    Color:
                        rgba: self.background_color
                    Rectangle:
                        size: self.size
                        pos: self.pos
            LabelH3:
                text: "NAME"
                pos_hint : {"x":0.05, "y":0.6}
                halign: 'center'
                background_color :182, 182, 182, 0.3
                canvas.before:
                    Color:
                        rgba: self.background_color
                    Rectangle:
                        size: self.size
                        pos: self.pos
            Spinner :
                id: spinner_main
                text: "Select Customer/Vendor"
                values: "Customer", "Vendor"
                on_text: root.customer_vendor_selection()
                size_hint: 0.54, 0.07
                pos_hint : {"x":0.45, "y":0.8}
                halign: 'center'
                background_color :1, 1, 0, 0.3 
                color : 1, 1, 1, 1
            Spinner :
                id: spinner_master
